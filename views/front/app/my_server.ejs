<div class="container mt-3">
    <div class="row">
        <% data_server.forEach(function (server) { %>
        <div class="col-sm-12 col-md-12 col-lg-12 col-xl-3">
            <div class="iq-card">
                <div class="iq-card-body">
                    <ul class="media-story m-0 p-0">
                        <li class="d-flex mb-4 align-items-center active">
                            <img src="<%- server.icon %>" alt="story-img" class="rounded-circle img-fluid">
                            <div class="stories-data ml-3">
                                <h5><%- server.name %></h5>
                                <p class="mb-0">not define</p>
                            </div>
                        </li>
                        <a href="/app/s/<%- server.server_id %>" class="btn btn-primary d-block"><i class="fad fa-cogs"></i> Setup</a>
                    </ul>
                </div>
            </div>
        </div>
        <% }) %>
    </div>
</div>