<div class="col-lg-12 row m-0 p-0">
    <div class="col-sm-6 col-lg-3">
        <div class="iq-card iq-card-block iq-card-stretch iq-card-height">
            <div class="iq-card-header d-flex justify-content-between">
                <div class="iq-header-title">
                    <h4 class="card-title">CPU usage</h4>
                </div>
            </div>
            <div class="iq-card-body pt-0">
                <div id="home-cpu-usage"></div>
                <div class="text-center">
                    <h4>NaN</h4>
                </div>
            </div>
        </div>
    </div>
    <div class="col-sm-6 col-lg-3">
        <div class="iq-card iq-card-block iq-card-stretch iq-card-height">
            <div class="iq-card-header d-flex justify-content-between">
                <div class="iq-header-title">
                    <h4 class="card-title">RAM usage</h4>
                </div>
            </div>
            <div class="iq-card-body pt-0">
                <div id="home-ram-usage"></div>
                <div class="text-center">
                    <h4>NaN</h4>
                </div>
            </div>
        </div>
    </div>
    <div class="col-sm-6 col-lg-3">
        <div class="iq-card iq-card-block iq-card-stretch iq-card-height">
            <div class="iq-card-header d-flex justify-content-between">
                <div class="iq-header-title">
                    <h4 class="card-title">DISK usage</h4>
                </div>
            </div>
            <div class="iq-card-body pt-0">
                <div id="home-disk-usage"></div>
                <div class="text-center">
                    <h4>NaN</h4>
                </div>
            </div>
        </div>
    </div>
    <div class="col-sm-6 col-lg-3">
        <div class="iq-card iq-card-block iq-card-stretch iq-card-height bg-primary rounded background-image-overlap">
            <div class="iq-card-body">
                <div class="d-flex align-items-center mb-3">
                    <div><img class="rounded" src="https://i.imgur.com/BIoz9ji.png" alt=""></div>
                    <h5 class="pl-3 text-white">incident</h5>
                </div>
                <p class="mb-2"><span class="text-danger">null</span> Error</p>
                <p class="mb-2"><span class="text-danger">null</span> Warning</p>
                <button type="submit" class="btn w-100 btn-white mt-4 text-primary viwe-more">Voir plus</button>
            </div>
        </div>
    </div>
    <div class="col-sm-3">
        <div class="iq-card iq-card-block iq-card-stretch iq-card-height-third h-auto">
            <div class="iq-card-body">
                <div class="row">
                    <div class="col-lg-12 text-center position-relative">
                        <div class="icon avatar-70 m-auto bg-success iq-border-success rounded-circle line-height-7 text-center">
                            <i class="ri-check-line"></i>
                        </div>
                    </div>
                    <div class="col-lg-12 mt-3 text-center">
                        <h5>WEB API</h5>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-sm-3">
        <div class="iq-card iq-card-block iq-card-stretch iq-card-height-third h-auto">
            <div class="iq-card-body">
                <div class="row">
                    <div class="col-lg-12 text-center position-relative">
                        <div class="icon avatar-70 m-auto bg-close iq-border-close rounded-circle line-height-7 text-center">
                            <i class="ri-close-line"></i>
                        </div>
                    </div>
                    <div class="col-lg-12 mt-3 text-center">
                        <h5>BOT STATUS</h5>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<%- username %>